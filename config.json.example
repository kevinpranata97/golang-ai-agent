{
  "server": {
    "port": "8080",
    "host": "0.0.0.0",
    "read_timeout": 30,
    "write_timeout": 30
  },
  "github": {
    "token": "",
    "webhook_secret": "",
    "base_url": "https://api.github.com"
  },
